{
  "name": "tead",
  "version": "0.3.0",
  "description": "Lighting the way to simpler testing",
  "files": ["src"],
  "main": "./src/index.js",
  "bin": {
    "tead": "./src/tead.js"
  },
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "@std/esm": "^0.16.0"
  },
  "scripts": {
    "format": "npx prettier --write \"{src,test}/**/*.js\"",
    "test": "node test",
    "test:coverage": "npx nyc --temp-directory coverage -r lcov -r text --all npm t",
    "test:watch": "node test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okwolf/tead.git"
  },
  "author": "Wolfgang Wedemeyer <wolf@okwolf.com>",
